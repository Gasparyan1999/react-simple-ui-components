{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "lib": [
        "ES2015","DOM"
      ],
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist", 
      "sourceMap": true,
      "declaration": true
    },
    "include": ["src"],
    "exclude": ["node_modules","dist"]
  }
  